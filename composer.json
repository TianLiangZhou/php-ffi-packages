{
    "name": "meshell/php-ffi-packages",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "zhoutianliang",
            "email": "mfkgdyve@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-ffi": "*"
    },
    "require-dev": {
        "overtrue/pinyin": "^4.0",
        "phpbench/phpbench": "^0.17.1",
        "phpunit/phpunit": "^9.5",
        "symplify/monorepo-builder": "^9.2"
    },
    "autoload": {
        "psr-4": {
            "FastFFI\\LAC\\": "packages/ffi-lac/src/",
            "FastFFI\\Opencc\\": "packages/ffi-opencc/src/",
            "FastFFI\\Pinyin\\": "packages/ffi-pinyin/src/",
            "FastFFI\\QrCode\\": "packages/ffi-qrcode/src/"
        }
    },
    "version": "1.0.0",
    "replace": {
        "meshell/ffi-lac": "1.0.3",
        "meshell/ffi-opencc": "1.0.3",
        "meshell/ffi-pinyin": "1.0.3",
        "meshell/ffi-qrcode": "1.0.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
